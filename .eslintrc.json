{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jquery": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-new-object": "off",
        "no-new": "error",
        "no-console": "off",
        "no-unused-vars": "warn",
        "no-useless-escape":"warn"
    }
}